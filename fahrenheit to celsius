//Write A C Program to Convert Temperature From Fahrenheit to Celsius 

#include<stdio.h>
int main()
{
  float c, f;
  printf("Enter Temperature in Fahrenheit : ");
  scanf("%f",&f);

  //Converting Fahrenheit into Celsius
  c = ((f-32)*5)/9;

  printf("Temperature in Celsius is : %f",c);
  return 0;
}
